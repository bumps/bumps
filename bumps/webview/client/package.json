{
  "name": "bumps-webview-client",
  "version": "0.1.21",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/bumps/bumps",
    "directory": "bumps/webview/client"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir -m development",
    "build_prod": "vite build --emptyOutDir -m production",
    "preview": "vite preview --port 4173",
    "format": "prettier --write \"src/**/*.{vue,ts,js,css,scss,json,yaml,svg,md}\" \"*.json\"",
    "lint": "eslint src --fix && prettier --write \"src/**/*.{vue,ts,js,css,scss,json,yaml,svg,md}\" \"*.json\"",
    "test:lint": "eslint src && prettier --check \"src/**/*.{vue,ts,js,css,scss,json,yaml,svg,md}\" \"*.json\"",
    "test:types": "vue-tsc --noEmit --skipLibCheck -p tsconfig.json --composite false"
  },
  "dependencies": {
    "bootstrap": "^5.3.7",
    "date-fns": "^4.1.0",
    "json-difference": "^1.16.1",
    "mpld3": "^0.5.10",
    "plotly.js": "^3.0.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "vue": "^3.5.17",
    "vue-json-pretty": "^2.5.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/bootstrap": "^5.2.10",
    "@types/plotly.js": "^2.35.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-vue": "^10.2.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "prettier": "^3.6.0",
    "prettier-plugin-css-order": "^2.1.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.2.10"
  },
  "trustedDependencies": [
    "es5-ext"
  ]
}
